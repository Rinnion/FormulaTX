            <size android:width="@dimen/program_square" android:height="@dimen/program_square"/>
            <stroke android:width="1dp" android:color="@android:color/holo_green_dark"/>
        </shape>
    </item>
    <item android:top="3dp" android:left="3dp" android:right="3dp" android:bottom="3dp">
        <shape android:shape="rectangle" >
            <solid android:color="@android:color/holo_green_dark"/>
        </shape>
    </item>
