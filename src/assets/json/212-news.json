\\u043e\\u043d \\u0438 PR-\\u0434\\u0438\\u0440\\u0435\\u043a\\u0442\\u043e\\u0440\\u043e\\u043c \\u0412\\u0435\\u0440\\u043e\\u0439 \\u041f\\u0443\\u0442\\u0438\\u043d\\u043e\\u0439, \\u043a\\u043e\\u0442\\u043e\\u0440\\u044b\\u0435 \\u0441\\u043c\\u043e\\u0433\\u043b\\u0438 \\u0441\\u0434\\u0435\\u043b\\u0430\\u0442\\u044c \\u0438\\u0437 \\u043f\\u0435\\u0442\\u0435\\u0440\\u0431\\u0443\\u0440\\u0436\\u0441\\u043a\\u043e\\u0433\\u043e \\u0442\\u0443\\u0440\\u043d\\u0438\\u0440\\u0430 \\u0441\\u043f\\u043e\\u0440\\u0442\\u0438\\u0432\\u043d\\u043e\\u0435 \\u0441\\u043e\\u0431\\u044b\\u0442\\u0438\\u0435 \\u0434\\u0435\\u0439\\u0441\\u0442\\u0432\\u0438\\u0442\\u0435\\u043b\\u044c\\u043d\\u043e \\u043c\\u0438\\u0440\\u043e\\u0432\\u043e\\u0433\\u043e \\u0443\\u0440\\u043e\\u0432\\u043d\\u044f.\\u041c\\u0435\\u0440\\u043e\\u043f\\u0440\\u0438\\u044f\\u0442\\u0438\\u044f, \\u043f\\u043e\\u0434\\u043e\\u0431\\u043d\\u044b\\u0435 St. Petersburg Open, \\u044f\\u0432\\u043b\\u044f\\u044e\\u0442\\u0441\\u044f \\u0437\\u043d\\u0430\\u0447\\u0438\\u043c\\u044b\\u043c\\u0438 \\u0434\\u043b\\u044f \\u043b\\u044e\\u0431\\u043e\\u0433\\u043e \\u0433\\u043e\\u0441\\u0443\\u0434\\u0430\\u0440\\u0441\\u0442\\u0432\\u0430. \\u042d\\u0442\\u043e \\u0438\\u043c\\u0438\\u0434\\u0436 \\u0441\\u0442\\u0440\\u0430\\u043d\\u044b, \\u0435\\u0435 \\u0441\\u043f\\u043e\\u0440\\u0442\\u0438\\u0432\\u043d\\u043e\\u0439 \\u0436\\u0438\\u0437\\u043d\\u0438, \\u044d\\u0442\\u043e \\u043d\\u043e\\u0432\\u044b\\u0435 \\u0442\\u0443\\u0440\\u0438\\u0441\\u0442\\u0438\\u0447\\u0435\\u0441\\u043a\\u0438\\u0435 \\u043f\\u043e\\u0442\\u043e\\u043a\\u0438 \\u0438 \\u0438\\u043d\\u0442\\u0435\\u0440\\u0435\\u0441 \\u0441\\u043e \\u0441\\u0442\\u043e\\u0440\\u043e\\u043d\\u044b \\u043c\\u0438\\u0440\\u043e\\u0432\\u043e\\u0439 \\u043e\\u0431\\u0449\\u0435\\u0441\\u0442\\u0432\\u0435\\u043d\\u043d\\u043e\\u0441\\u0442\\u0438. \\u0414\\u043e\\u043f\\u043e\\u043b\\u043d\\u0438\\u0442\\u0435\\u043b\\u044c\\u043