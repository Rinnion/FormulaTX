38\\u043b\\u0438 \\u043c\\u0430\\u043a\\u0441\\u0438\\u043c\\u0430\\u043b\\u044c\\u043d\\u044b\\u0435 \\u043e\\u0446\\u0435\\u043d\\u043a\\u0438 \\u043f\\u043e \\u0432\\u0441\\u0435\\u043c \\u043a\\u0440\\u0438\\u0442\\u0435\\u0440\\u0438\\u044f\\u043c, \\u0441\\u043e\\u043e\\u0442\\u0432\\u0435\\u0442\\u0441\\u0442\\u0432\\u0443\\u044e\\u0449\\u0438\\u0435 \\u0438\\u0445 \\u043a\\u0430\\u0442\\u0435\\u0433\\u043e\\u0440\\u0438\\u044f\\u043c. \\u041a\\u0430\\u0436\\u0434\\u044b\\u0439 \\u0438\\u0437 \\u044d\\u0442\\u0438\\u0445 \\u0442\\u0443\\u0440\\u043d\\u0438\\u0440\\u043e\\u0432 \\u2013 \\u0432\\u044b\\u0434\\u0430\\u044e\\u0449\\u0438\\u0435\\u0441\\u044f \\u0441\\u043f\\u043e\\u0440\\u0442\\u0438\\u0432\\u043d\\u044b\\u0435 \\u0441\\u043e\\u0431\\u044b\\u0442\\u0438\\u044f. \\u042d\\u0442\\u0430 \\u043f\\u0440\\u0435\\u043c\\u0438\\u044f \\u043f\\u043e\\u0434\\u0442\\u0432\\u0435\\u0440\\u0436\\u0434\\u0430\\u0435\\u0442 \\u043f\\u0440\\u0438\\u0437\\u043d\\u0430\\u043d\\u0438\\u0435 \\u0441\\u043e \\u0441\\u0442\\u043e\\u0440\\u043e\\u043d\\u044b \\u0438\\u0433\\u0440\\u043e\\u043a\\u043e\\u0432, \\u043a\\u043e\\u0442\\u043e\\u0440\\u044b\\u0435 \\u0433\\u043e\\u043b\\u043e\\u0441\\u043e\\u0432\\u0430\\u043b\\u0438. \\u0414\\u0438\\u0440\\u0435\\u043a\\u0446\\u0438\\u044f\\u043c\\u0438 \\u0442\\u0435\\u043d\\u043d\\u0438\\u0441\\u043d\\u044b\\u0445 \\u0442\\u0443\\u0440\\u043d\\u0438\\u0440\\u043e\\u0432 \\u043f\\u0440\\u043e\\u0434\\u0435\\u043b\\u0430\\u043d\\u0430 \\u043e\\u0433\\u0440\\u043e\\u043c\\u043d\\u0430\\u044f \\u0440\\u0430\\u0431\\u043e\\u0442\\u0430, \\u0447\\u0442\\u043e\\u0431\\u044b \\u0434\\u043e\\u0441\\u0442\\u0438\\u0447\\u044c \\u0442\\u0430\\u043a\\u043e\\u0433\\u043e \\u0432\\u044b\\u0441\\u043e\\u043a\\u043e\\u0433\\u043e \\u043a\\u043b\\u0430\\u0441\\u0441\\u0430 \\u043e\\u0440\\u0433\\u0430\\u043d\\u0438\\u0437\\u0430\\u0446\\u0438\\u0438, \\u043a\\u043e\\u0442\\u043e\\u0440\\u0430\\u044f \\u043f\\u043e\\u0437\\u0432\\u043e\\u043